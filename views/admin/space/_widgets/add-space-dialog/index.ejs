<el-dialog v-model="visibleAddSpace" title="添加空间" width="400px">
    <el-form :model="formAddSpace" label-width="100px">
        <el-form-item label="空间名称">
            <el-input v-model="formAddSpace.space_name" placeholder="空间名称" autocomplete="off"></el-input>
        </el-form-item>
        <el-form-item label="空间标识">
            <el-input v-model="formAddSpace.space_mark" placeholder="空间标识" autocomplete="off"></el-input>
        </el-form-item>
        <el-form-item label="空间排序">
            <el-input-number class="space-add-number" v-model="formAddSpace.sort" :min="0" :step="1" step-strictly></el-input-number>
        </el-form-item>
    </el-form>
    <template #footer>
      <span class="dialog-footer">
        <el-button @click="visibleAddSpace = false">取消</el-button>
        <el-button type="primary" @click="addSpaceSubmit()">
          确认添加
        </el-button>
      </span>
    </template>
</el-dialog>
