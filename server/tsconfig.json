{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2015",
    "module": "ES2015",
    "strict": true,
    "allowJs": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "paths": {
      "@/*": ["src/*"]
    },
    "outDir": "dev"
  }
}
